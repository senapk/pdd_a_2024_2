class Animal {
    especie: string;
    estagio: number;
    barulho: string;

    constructor(especie: string, barulho: string) {
        this.especie = especie;
        this.barulho = barulho;
    }
    toString(): string {
        return `${this.especie}:${this.estagio}:${this.barulho}`
    }
}


